{
  "name": "@blackbox_ai/ai-logs-watcher",
  "version": "0.1.9",
  "description": "AI-powered log monitoring and alerting system with SMS notifications via Twilio",
  "main": "./dist/main.index.js",
  "module": "./dist/main.index.js",
  "types": "./dist/main.index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./Ailogw": {
      "types": "./dist/Ailogw.d.ts",
      "import": "./dist/Ailogw.js",
      "require": "./dist/Ailogw.js"
    },
    "./exceptions": {
      "types": "./dist/exceptions/index.d.ts",
      "import": "./dist/exceptions/index.js",
      "require": "./dist/exceptions/index.js"
    },
    "./voice": {
      "types": "./dist/voice/index.d.ts",
      "import": "./dist/voice/index.js",
      "require": "./dist/voice/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "ai",
    "logs",
    "monitoring",
    "alerting",
    "elevenlabs",
    "sms",
    "twilio",
    "log-analysis",
    "artificial-intelligence",
    "typescript"
  ],
  "author": {
    "name": "blackboxai",
    "url": "https://www.blackbox.ai/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blackboxaicode/AI-logs-watcher.git"
  },
  "bugs": {
    "url": "https://github.com/blackboxaicode/AI-logs-watcher/issues"
  },
  "homepage": "https://github.com/blackboxaicode/AI-logs-watcher#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "^1.2.23",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.18.1",
    "esbuild-plugin-file-path-extensions": "^2.1.4",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {},
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.15.0",
    "@openai/agents": "^0.1.6",
    "dotenv": "^17.2.2",
    "elevenlabs": "^1.59.0",
    "esbuild": "^0.25.9",
    "nanoid": "^5.1.5",
    "openai": "^5.6.0",
    "twilio": "^5.9.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm install && tsx build.ts",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "config-elevenlabs": "tsx createElevenLabsConfig.ts"
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.25.10"
  }
}
